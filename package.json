{
  "name": "postcss-prepend-url-imports",
  "version": "0.0.0",
  "description": "PostCSS plugin that prepends URL imports at the beginning of a stylesheet.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "css-import"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest-ci --coverage && eslint-ci ."
  },
  "author": "Raffy Alcoriza <opensource@raffyal.co>",
  "license": "MIT",
  "repository": "picapixa/postcss-prepend-url-imports",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "postcss": "^7.0.35"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "clean-publish": "^1.1.8",
    "eslint": "^7.10.0",
    "eslint-ci": "^1.0.0",
    "eslint-plugin-jest": "^24.0.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-ci": "^0.1.1",
    "jest-cli": "^26.4.2",
    "lint-staged": "^10.4.0",
    "postcss": "^7.0.35",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  }
}
